      "strokeWidth": 1,
      "strokeStyle": "solid",
      "roughness": 1,
      "opacity": 100,
      "angle": 0,
      "x": 87123.9979331293,
      "y": 99483.21922854992,
      "strokeColor": "#000000",
      "backgroundColor": "transparent",
      "width": 914,
